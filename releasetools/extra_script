mount("ext4", "EMMC", "/dev/block/platform/mtk-msdc.0/by-name/system", "/system", "");
set_metadata("/system/etc/init.fosflags.sh", "uid", 0, "gid", 0, "mode", 0755, "selabel", "u:object_r:system_file:s0");
ui_print("Patching for TWRP...");
set_metadata("/system/etc/recovery/ramdisk-recovery.cpio.lzma", "uid", 0, "gid", 0, "mode", 0644, "selabel", "u:object_r:system_file:s0");
set_metadata("/system/etc/recovery/2ndinit", "uid", 0, "gid", 0, "mode", 0755, "selabel", "u:object_r:system_file:s0");
set_metadata("/system/bin/ext4_resize", "uid", 0, "gid", 0, "mode", 0755, "selabel", "u:object_r:system_file:s0");
unmount("/system");

